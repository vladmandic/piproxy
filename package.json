{
  "name": "@vladmandic/piproxy",
  "version": "1.1.23",
  "description": "PiProxy: Reverse Proxy in NodeJS for HTTP/HTTPS/HTTP2 with SSL Certificate Management, Compression, Security Enforcement and Rate Limiting",
  "main": "server/piproxy.js",
  "sideEffects": false,
  "scripts": {
    "start": "node --unhandled-rejections=strict server/piproxy.js",
    "setup": "node ./setup.js",
    "lint": "eslint *.json *.js server/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/piproxy.git"
  },
  "keywords": [
    "proxy",
    "reverse-proxy",
    "http2",
    "letsencrypt",
    "geoip",
    "noip",
    "rate-limiter",
    "brotli",
    "nodejs"
  ],
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladmandic/piproxy/issues"
  },
  "homepage": "https://github.com/vladmandic/piproxy#readme",
  "engines": {
    "node": "^12.0.0"
  },
  "dependencies": {
    "@vladmandic/piacme": "^0.1.30",
    "@vladmandic/pilogger": "^0.4.4",
    "connect": "^3.7.0",
    "helmet": "^5.0.2",
    "maxmind": "^4.3.6",
    "nedb-promises": "^6.0.3",
    "simple-git": "^3.4.0",
    "superagent": "^7.1.1"
  },
  "devDependencies": {
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0"
  }
}
