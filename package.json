{
  "name": "@vladmandic/piproxy",
  "version": "2.0.0",
  "description": "Reverse HTTP/HTTPS/HTTP2 Proxy in NodeJS with SSL Certificate Management, Compression, Security Enforcement and Rate Limiting",
  "main": "dist/piproxy.js",
  "sideEffects": false,
  "scripts": {
    "build": "build --profile production",
    "start": "node --unhandled-rejections=throw dist/piproxy.js",
    "dev": "build --profile production && node --trace-uncaught --trace-warnings --unhandled-rejections=strict --enable-source-maps dist/piproxy.js",
    "lint": "eslint *.json src/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/piproxy.git"
  },
  "keywords": [
    "proxy",
    "reverse-proxy",
    "http2",
    "letsencrypt",
    "geoip",
    "noip",
    "rate-limiter",
    "brotli",
    "nodejs"
  ],
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladmandic/piproxy/issues"
  },
  "homepage": "https://github.com/vladmandic/piproxy#readme",
  "engines": {
    "node": "^12.0.0"
  },
  "dependencies": {
    "@vladmandic/piacme": "^1.0.2",
    "connect": "^3.7.0",
    "helmet": "^5.1.1",
    "maxmind": "^4.3.6",
    "simple-git": "^3.12.0",
    "superagent": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vladmandic/build": "^0.7.9",
    "@vladmandic/pilogger": "^0.4.6",
    "esbuild": "^0.15.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "tslib": "^2.4.0",
    "typescript": "4.7.4"
  }
}
