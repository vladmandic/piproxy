{
  "name": "@vladmandic/piproxy",
  "version": "1.1.18",
  "description": "PiProxy: Reverse Proxy in NodeJS for HTTP/HTTPS/HTTP2 with SSL Certificate Management, Compression, Security Enforcement and Rate Limiting",
  "main": "server/piproxy.js",
  "sideEffects": false,
  "scripts": {
    "start": "node --unhandled-rejections=strict  --no-deprecation server/piproxy.js",
    "setup": "node ./setup.js",
    "lint": "eslint *.json *.js server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/piproxy.git"
  },
  "keywords": [
    "proxy",
    "reverse-proxy",
    "http2",
    "letsencrypt",
    "geoip",
    "noip",
    "rate-limiter",
    "brotli",
    "nodejs"
  ],
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladmandic/piproxy/issues"
  },
  "homepage": "https://github.com/vladmandic/piproxy#readme",
  "engines": {
    "node": "^12.0.0"
  },
  "dependencies": {
    "@vladmandic/piacme": "^0.1.28",
    "@vladmandic/pilogger": "^0.2.17",
    "connect": "^3.7.0",
    "helmet": "^4.5.0",
    "maxmind": "^4.3.1",
    "nedb-promises": "^4.1.2",
    "simple-git": "^2.38.0",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1"
  }
}
