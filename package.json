{
  "name": "@vladmandic/piproxy",
  "version": "1.1.1",
  "description": "PiProxy: NodeJS Web Proxy",
  "main": "server/piproxy.js",
  "sideEffects": false,
  "scripts": {
    "start": "node server/piproxy.js",
    "setup": "./setup.js",
    "lint": "eslint server/*.js *.json && depcheck --ignores='eslint-plugin-*,auditjs,npm-check-updates' && auditjs ossi --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/piproxy.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "nodejs",
    "proxy",
    "http2",
    "letsencrypt",
    "geoip",
    "rate-limiter",
    "brotli"
